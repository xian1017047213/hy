<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="HShopPropertyValue" table="h_shop_property_value">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="createTime" type="datetime" column="create_time" nullable="true"/>
    <field name="modifyTime" type="datetime" column="modify_time" nullable="true"/>
    <field name="propertyId" type="integer" column="property_id" nullable="true"/>
    <field name="sequence" type="integer" column="sequence" nullable="true"/>
    <field name="value" type="string" column="value" length="500" nullable="true"/>
    <field name="version" type="datetime" column="version" nullable="true"/>
  </entity>
</doctrine-mapping>
